{
  "name": "easyclass-webclient",
  "version": "1.0.0",
  "description": "Easy Class Web Client",
  "main": "index.js",
  "scripts": {
    "build": "rimraf build && webpack --config ./config/webpack.production.babel.js",
    "start": "webpack-dev-server --config ./config/webpack.development.babel.js",
    "test": "jest --verbose --watchAll",
    "test-ci": "jest --verbose"
  },
  "jest": {
    "globals": {
      "SERVER_URL": "http://localhost:8081"
    },
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "author": "Rafael Companhoni",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "autoprefixer": "^7.1.5",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.4",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "lodash": "^4.17.5",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-css-modules": "^4.7.6",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-time-ago": "^2.0.3",
    "react-typist": "^2.0.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.2",
    "style-loader": "^0.19.0",
    "url-loader": "^1.0.1",
    "uuid": "^3.3.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.4"
  },
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.1",
    "enzyme-to-json": "^3.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "react-test-renderer": "^16.4.2"
  }
}
